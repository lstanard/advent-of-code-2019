{
  "extends": "tslint:recommended",
  "rules": {
      "max-line-length": {
          "options": [90]
      },
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "no-console": {
          "severity": "warning",
          "options": ["debug", "info", "time", "timeEnd", "trace"]
      },
      "trailing-comma": false,
      "array-type": [true, "generic"]
  },
  "jsRules": {
      "max-line-length": {
          "options": [90]
      }
  }
}